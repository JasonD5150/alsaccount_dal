<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering
        SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <!-- Set table-filters when using the Hibernate Tools Ant target to specify reveng tables.  If not
         set, all tables accessible by the user specified in the hibernate.reveng.cfg.xml file will be
         reverse engineered.  -->
    <table-filter match-name="ALS_PERSON_ITEM_TEMPL_LINK" exclude="false"/>

    <table name="ALS_PERSON_ITEM_TEMPL_LINK" schema="ALS" class="fwp.alsaccount.dao.refund.AlsPersonItemTemplLink">
        <primary-key id-class="fwp.alsaccount.dao.refund.AlsPersonItemTemplLinkIdPk" property="idPk">
            <key-column name="APT_APP_TYPE_CD" jdbc-type="NUMERIC" type="Integer"></key-column>
            <key-column name="APT_DATA_ENTRY" jdbc-type="NUMERIC" type="Integer"></key-column>
            <key-column name="APITL_ITEM_TYPE_CD" jdbc-type="VARCHAR" type="String"></key-column>
        </primary-key>
        <column name="APITL_WHEN_LOG" jdbc-type="DATE" type="java.sql.Timestamp"></column>
        <column name="APITL_WHO_LOG" jdbc-type="VARCHAR" type="String"></column>
    </table>

    <table name="ALS_ITEM_TEMPLATE_MASTER" schema="ALS" class="fwp.alsaccount.dao.refund.AlsItemTemplateMaster">
        <primary-key>
            <key-column name="AITM_ITEM_TEMPLATE_CD" jdbc-type="NUMERIC" type="Integer" />
        </primary-key>
        <column name="AITM_TEMPLATE_DESC" jdbc-type="VARCHAR" type="String"></column>
        <column name="AITM_WHO_LOG" jdbc-type="VARCHAR" type="String"></column>
        <column name="AITM_WHEN_LOG" jdbc-type="DATE" type="java.sql.Timestamp"></column>
    </table>
</hibernate-reverse-engineering>